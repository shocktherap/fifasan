  <tr>
    <td><?=$number+=1;?></td>
    <td>
      <?php $keterangan = $this->get_data->get_sub_by($key->subpekerjaan_id);
        $data['id_project'] = $id_project;
        $data['key'] = $key;
        echo $keterangan->nama;
      ?>
    </td>
    <td><a data-toggle="modal" href="#myModal<?=$key->id;?>"><i class="icon-trash"></i>Delete</a></td>
    <?php 
      $this->load->view('subproject/modal_delete_subproject', $data);
    ?>
  </tr>
<? } ?>
</table>

